<!DOCTYPE html>
<html ng-app="employeeManagement">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Employee Management Portal</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <script src="https://npmcdn.com/tether@1.2.4/dist/js/tether.min.js"></script>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
</head>
<body>
    

    <div class="container body-content" style="margin-top: 30px;">
        <div style="min-height:500px;">
            @RenderBody()
        </div>
        <hr />
        <footer class="pull-left">
            <p>&copy; @DateTime.Now.Year - Employee Management Portal</p>
        </footer>
    </div>

    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/angular.min.js"></script>
    <script src="~/Scripts/ng-file-upload-shim.min.js"></script>
    <script src="~/Scripts/ng-file-upload.min.js"></script>
    <script src="~/app/Common/module.js"></script>
    <script src="~/app/Common/AuthInterceptorService.js"></script>
    @*<script src="~/Scripts/angular-route.min.js"></script>*@
    <script src="~/Scripts/angula_1_router.js"></script>
    <script src="~/app/Common/MainRoute.js"></script>
    <script src="~/app/Employee/AddEmployee/AddEmployee.js"></script>
    <script src="~/app/Employee/ListEmployee/EmployeeList.js"></script>
    <script src="~/app/Employee/EmployeeDetail/EmployeeDetail.js"></script>
    <script src="~/app/Security/login/login.js"></script>
</body>
</html>